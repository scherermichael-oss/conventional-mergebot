{
  "name": "conventional-mergebot",
  "version": "0.0.0",
  "description": "Squashes and merges pull requests with a Conventional Commit message.",
  "author": {
    "name": "Michael Scherer",
    "email": "mail@scherermichael.net"
  },
  "repository": "https://github.com/scherermichael/conventional-mergebot",
  "main": "index.js",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "lint": "standard",
    "start": "probot run ./index.js",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest . --watch"
  },
  "standard": {
    "globals": [
      "jest",
      "it",
      "expect",
      "describe"
    ]
  },
  "keywords": [
    "conventional-commits",
    "probot",
    "probot-plugin",
    "semver"
  ],
  "license": "MIT",
  "dependencies": {
    "@probot/serverless-lambda": "github:probot/serverless-lambda",
    "@semantic-release/commit-analyzer": "5.0.3",
    "debug": "3.1.0",
    "probot": "6.1.0",
    "serverless": "1.29.2",
    "serverless-offline": "3.25.6"
  },
  "devDependencies": {
    "jest": "22.4.3",
    "localtunnel": "1.9.0",
    "smee-client": "1.0.1",
    "standard": "11.0.1"
  },
  "jest": {
    "testRegex": "/test/unit/.+\\.test\\.js$",
    "testURL": "http://localhost/"
  },
  "engines": {
    "node": "8"
  }
}
